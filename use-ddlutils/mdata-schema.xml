      <column name="updateDate" primaryKey="false" required="false" type="TIMESTAMP" size="19" autoIncrement="false" description="更新时间"/>
      <column name="del_flag" primaryKey="false" required="false" type="CHAR" size="1" default="0" autoIncrement="false" description="删除标志"/>
      <column name="remarks" primaryKey="false" required="false" type="VARCHAR" size="255" autoIncrement="false" description="备注"/>
    </table>

